{
  "name": "@codetanzania/ewea-common",
  "version": "0.12.0",
  "description": "Common utilities for EWEA",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "posttest": "rimraf test/fixtures/*.spec.csv",
    "test": "NODE_ENV=test LOGGER_LOG_ENABLED=false DEFAULT_LOCALE=en LOCALES=en,sw nyc --reporter=html --reporter=text mocha --exit --timeout=80000 --require @babel/register test/**/*.spec.js",
    "test:unit": "NODE_ENV=test npm run pretest && NODE_ENV=test DEFAULT_LOCALE=en LOCALES=en,sw mocha --exit --timeout=80000 --require @babel/register test/unit/**/*.spec.js",
    "test:integration": "NODE_ENV=test npm run pretest && NODE_ENV=test DEFAULT_LOCALE=en LOCALES=en,sw mocha --exit --timeout=80000 --require @babel/register test/integration/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "commit": "git-cz",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog",
    "chore:deps": "ncu -u && npm install --save",
    "lint:config": "eslint --print-config rollup.config.js",
    "dev": "npm run seed && npm run start",
    "start": "NODE_ENV=development BASE_PATH=./examples babel-node ./examples/app.js",
    "seed": "NODE_ENV=development BASE_PATH=./examples babel-node ./examples/seed.js"
  },
  "keywords": [
    "codetanzania",
    "ewea",
    "ewea-module",
    "common",
    "helpers",
    "utilities",
    "constants",
    "defaults",
    "disaster",
    "hazard",
    "risk",
    "emergency",
    "incident",
    "activation",
    "execution",
    "action",
    "task",
    "stakeholder",
    "assessment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codetanzania/ewea-common.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codetanzania/ewea-common/issues",
    "email": "lallyelias87@gmail.com"
  },
  "homepage": "https://github.com/codetanzania/ewea-common",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@benmaruchu/faker": "^4.3.0",
    "@codetanzania/ewea-event": "^0.10.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/travis-cli": "^8.3.5",
    "@lykmapipo/express-test-helpers": "^0.9.2",
    "@lykmapipo/mongoose-test-helpers": "^0.7.45",
    "@lykmapipo/test-helpers": "^0.5.18",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "csv-parse": "^4.9.0",
    "csv-to-array": "^1.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "doxdox": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^24.0.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "lodash.template": "^4.5.0",
    "mocha": "^7.1.2",
    "mongoose": ">=5.9.12",
    "npm-check-updates": "^4.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.6",
    "shelljs": "^0.8.4"
  },
  "peerDependencies": {
    "mongoose": ">=5.9.12"
  },
  "dependencies": {
    "@codetanzania/ewea-internals": ">=0.13.0",
    "@codetanzania/emis-stakeholder": ">=2.6.0",
    "@lykmapipo/common": ">=0.33.1",
    "@lykmapipo/constants": ">=0.4.12",
    "@lykmapipo/env": ">=0.17.4",
    "@lykmapipo/express-common": ">=0.18.2",
    "@lykmapipo/express-rest-actions": ">=0.8.14",
    "@lykmapipo/file": ">=0.1.24",
    "@lykmapipo/geo-tools": ">=0.7.1",
    "@lykmapipo/logger": ">=0.5.12",
    "@lykmapipo/mongoose-common": ">=0.33.1",
    "@lykmapipo/mongoose-exportable": ">=0.3.28",
    "@lykmapipo/mongoose-sequenceable": ">=0.2.14",
    "@lykmapipo/permission": ">=0.8.26",
    "@lykmapipo/postman": ">=0.18.10",
    "@lykmapipo/predefine": ">=1.15.1",
    "async": ">=3.2.0",
    "lodash": ">=4.17.15",
    "mongoose-geojson-schemas": ">=0.12.1",
    "mongoose-locale-schema": ">=0.4.20",
    "mongoose-rest-actions": ">=0.29.22"
  },
  "engines": {
    "node": ">=12.4.0",
    "npm": ">=6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
